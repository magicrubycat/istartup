<div class="container">
<h2>Welcome to your Dashboard!</h2>

<div class="container">
    <%= render 'card-grid-interests' %>

    <ul>
      <% @interests.each do |interest| %>
       <li> <%= interest.name %> </li>
      <% end %>
    </ul>
  <!-- SAFE THIS FOR LATER  <%= link_to "Edit interests", sectors_path %> -->
</div>

<div class="container">
  <div class="favorites">
    <%= render 'card-grid-favorites' %>

    <% if current_user.photo.attached? %>
    <%= cl_image_tag current_user.photo.key, height: 40, width: 40, crop: :fill %>
    <% end %>
    <ul>
      <% @favorites.each do |favorite| %>
      <li> <%= link_to favorite.name, startup_path(favorite) %> </li>
      <% end %>
    </ul>
  </div>
</div>

<div class="applications">
  <%= render 'card-grid-applications' %>
</div>
</div>
