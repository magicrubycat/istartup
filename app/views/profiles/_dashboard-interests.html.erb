<div class="container interests">
  <h2>You are interested in</h2>
  <p style="color: #52A87C" >______________________________________</p>

  <div class="card-grid-interests">
    <% @interests.each do |interest| %>
    <div class="card-interests">
      <div class= "card-logo-interests">
        <i class="<%= startup.sector_icon_font %>"></i>
      </div>
      <h6><%= interest.name %></h6>
    </div>
    <% end %>
  </div>

</div>
